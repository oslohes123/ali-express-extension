{
    "manifest_version": 3,
    "name": "AliExpress Total Price Calculator",
    "description": "This extension calculates the total price of the products in the cart on Aliexpress with the number of items",
    "browser_specific_settings": {
        "gecko": {
            "id": "aliexpress-total-price-calculator@example.com",
            "strict_min_version": "58.0"
        }
    },
    "version": "1.1",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icons/icon32.png",
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}